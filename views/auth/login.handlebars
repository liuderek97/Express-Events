<h1> LOG IN </h1>
{{#if error }}
    <p>{{error}}</p>
{{/if}}

<form action="/login" method="POST">
    <label>Email</label>
    <input name="email" type="text" />
    <label> Password </label>
    <input name="password" type="password" />
    <input type="submit" value="Submit" />
</form>
<p><a href="/register">Register</a></p>